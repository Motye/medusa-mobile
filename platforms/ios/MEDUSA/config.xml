<?xml version='1.0' encoding='utf-8'?>
<widget id="org.trmn.medusa" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="AppVersion">
        <param name="ios-package" value="AppVersion" />
    </feature>
    <feature name="SpinnerPlugin">
        <param name="ios-package" value="SpinnerPlugin" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="YoikScreenOrientation">
        <param name="ios-package" value="YoikScreenOrientation" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="CDVBarcodeScanner" />
    </feature>
    <name>MEDUSA</name>
    <description>
        Mobile client for The Royal Manticoran Navy: The Official Honor Harrington Fan Organization
    </description>
    <author email="David.L.Weiner30030@gmail.com">
        David Weiner
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="res/ios/project-medusa-180.png" width="180" />
    <icon height="167" src="res/ios/project-medusa-167.png" width="167" />
    <icon height="152" src="res/ios/project-medusa-152.png" width="152" />
    <icon height="87" src="res/ios/project-medusa-87.png" width="87" />
    <icon height="80" src="res/ios/project-medusa-80.png" width="80" />
    <icon height="76" src="res/ios/project-medusa-76.png" width="76" />
    <icon height="60" src="res/ios/project-medusa-60.png" width="60" />
    <icon height="58" src="res/ios/project-medusa-58.png" width="58" />
    <icon height="29" src="res/ios/project-medusa-29.png" width="29" />
    <splash height="180" src="res/ios/project-medusa-180.png" width="180" />
    <splash height="167" src="res/ios/project-medusa-167.png" width="167" />
    <splash height="152" src="res/ios/project-medusa-152.png" width="152" />
    <splash height="87" src="res/ios/project-medusa-87.png" width="87" />
    <splash height="80" src="res/ios/project-medusa-80.png" width="80" />
    <splash height="76" src="res/ios/project-medusa-76.png" width="76" />
    <splash height="60" src="res/ios/project-medusa-60.png" width="60" />
    <splash height="58" src="res/ios/project-medusa-58.png" width="58" />
    <splash height="29" src="res/ios/project-medusa-29.png" width="29" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="CameraUsesGeolocation" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="NSPhotoLibraryUsageDescription" value="Store ID card and other related images" />
    <preference name="orientation" value="all" />
</widget>
